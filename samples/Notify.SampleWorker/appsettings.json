{
  "Notify": {
    "QueuePrefix": "notify-sample",
    "Email": {
      "Concurrency": 1,
      "Prefetch": 10,
      "BatchSize": 10,
      "BatchMaxWaitMs": 500
    },
    "Sms": {
      "Concurrency": 1,
      "Prefetch": 10,
      "BatchSize": 10,
      "BatchMaxWaitMs": 500
    },
    "Push": {
      "Concurrency": 1,
      "Prefetch": 10,
      "BatchSize": 10,
      "BatchMaxWaitMs": 500
    },
    "Providers": {
      "Email": {
        "ProviderName": "SampleEmail",
        "DefaultRecipient": "email@example.com"
      },
      "Sms": {
        "ProviderName": "SampleSms",
        "DefaultRecipient": "+15555550100"
      },
      "Push": {
        "ProviderName": "SamplePush",
        "DefaultRecipient": "push-device-id"
      }
    }
  },
  "RabbitMq": {
    "Host": "localhost",
    "Port": 5672,
    "Username": "guest",
    "Password": "guest",
    "VirtualHost": "/",
    "ExchangeName": "",
    "UseTls": false
  }
}
